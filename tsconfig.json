{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "lib": ["ESNext", "DOM"],
    "outDir": "dist",
    "baseUrl": "src",
    "moduleResolution": "node",
    "noEmit": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
